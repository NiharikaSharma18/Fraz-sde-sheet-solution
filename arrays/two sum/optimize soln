class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> v;
        unordered_map<int,int> m;
        for(int i=0;i<nums.size();i++){
            int c=target-nums[i];
             if(m.find(c)!=m.end())
        {return {m[target-nums[i]],i};
         
         }
        else 
        m[nums[i]]=i;
        }
    
        return {};
    }
};
