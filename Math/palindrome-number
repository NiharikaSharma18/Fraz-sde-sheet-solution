class Solution {
public:
    bool isPalindrome(int x) {
        int k=x;
        long long r=0;
        if(x<0)
        return false;
        while(x!=0){
            int d=x%10;
            r=r*10+d;
            x=x/10;
        }
        if(r==k)
        return true;
        else 
        return false;
    }
};
